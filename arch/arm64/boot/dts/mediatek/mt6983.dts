		i2c12 = &i2c12;
		i2c13 = &i2c13;
		ovl0 = &disp_ovl0;
		ovl3 = &disp_ovl0_2l;
		rdma0 = &disp_rdma0;
		dsi0 = &dsi0;
		scp_i2c1 = &scp_i2c1;
		tdshp0 = &disp_tdshp0;
		color0 = &disp_color0;
		ccorr0 = &disp_ccorr0_0;
		ccorr1 = &disp_ccorr0_1;
		c3d0 = &disp_c3d0;
